set autoindent
set autoread
set browsedir=bufferset clipboard=unnamed
set cmdheight=2
set expandtab
set hidden
set hlsearch
set incsearch
set laststatus=2
set list
set listchars=eol:$,tab:>\ ,extends:<
set nocompatible
set number
set ruler
set shiftwidth=2
set showcmd
set showmatch
set smartcase
set smartindent
set smarttab
set tabstop=2
set whichwrap=b,s,h,l,<,>,[,]



call pathogen#infect()
syntax on
filetype plugin indent on

